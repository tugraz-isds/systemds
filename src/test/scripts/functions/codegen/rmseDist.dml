U = matrix( 4, rows=4000, cols=10)
V = matrix( 5, rows=10, cols=2000)
X = U %*% V
X[1:3500,] = matrix(0,3500,2000);

while(FALSE){}

D = sqrt(-2 * U %*% V + rowSums(U^2) + colSums(V^2));
S = as.matrix(sum(rowSums((X != 0) * (X - D))^2));

write(S,$1)

