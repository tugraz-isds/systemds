X = read($1);
Y = read($2);

sum = 0;
tmpX = X[1,]
tmpY = Y[1,];
R = matrix(0, 1, nrow(X));

for (i in 2:nrow(X)) {
  Res1 = tmpX * tmpY;
  tmpX = rbind(tmpX, X[i,]);
  tmpY = rbind(tmpY, Y[i,]);
  while(FALSE) {}	
  R[1,i] = sum(Res1);
  sum = sum + sum(Res1);
}

write(R, $3, format="text");
