# junit test class: org.tugraz.sysds.test.integration.functions.unary.scalar.NegationTest.java

$$readhelper$$

TrueTrue = Helper;
booleanVar = TRUE;
while(!booleanVar == TRUE) {
	TrueTrue = 2 * Helper;
}
write(TrueTrue, "$$outdir$$true_true", format="text");

TrueFalse = Helper;
booleanVar = TRUE;
while(!booleanVar == FALSE) {
	TrueFalse = 2 * Helper;
	booleanVar = FALSE;
}
write(TrueFalse, "$$outdir$$true_false", format="text");

FalseFalse = Helper;
booleanVar = FALSE;
while(!booleanVar == FALSE) {
	FalseFalse = 2 * Helper;
}
write(FalseFalse, "$$outdir$$false_false", format="text");

FalseTrue = Helper;
booleanVar = FALSE;
while(!booleanVar == TRUE) {
	FalseTrue = 2 * Helper;
	booleanVar = TRUE;
}
write(FalseTrue, "$$outdir$$false_true", format="text");