cond = $1
R = as.matrix(list(0,0,0,-1));

# empty list construction
L = list();
if( cond & sum(R) > 0 )
  L = list(R, R);
R[1,] = length(L);

# list append
for(i in 1:7)
  L = append(L, as.matrix(i))
R[2,] = length(L);

# list remove
for(i in 1:4) {
  [L,x] = remove(L, 2);
	print(toString(as.matrix(x))); 
}
R[3,] = length(L);

write(R, $2);